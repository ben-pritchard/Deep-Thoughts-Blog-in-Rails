<%= render "layouts/errors", :object => @post %>

<%= form_for @post do |f| %>
  <%= f.label :title %>
  <br>
  <%= f.text_field :title %>
  <br>
  <%= f.label :entry %>
  <br>
  <%= f.text_area :entry, :rows => 10, :cols => "90" %>
  <%= f.file_field :photo %>
  <br>
  <%= f.submit "Submit", :class => "btn btn-success" %>
<% end %>

<br>

<%= link_to "Nevermind", root_path, :class => "btn btn-primary" %>
